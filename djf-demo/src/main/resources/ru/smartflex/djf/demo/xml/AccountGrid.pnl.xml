<?xml version="1.0" encoding="UTF-8"?>

<panel>
	<layout clazz="java.awt.BorderLayout" />
	<items>
		<grid constraint="Center" selectAble="fired" bindPref="pa" id="idAccountGrid">
			<cols>
				<int bind="idAccount" title="${demo.gridfields.grid.idAcc}"
					width="30" enabled="no" noResize="yes" />

				<text bind="accountNumber" title="${demo.gridfields.grid.an}"
					width="110:120" />
				<checkbox bind="hasChildren" title="${demo.gridfields.grid.hasChildren}"
					width="40:40" accessible="${form.itemsAccessBank}"
					enabled="yes" />
				<date bind="dateCreate" title="${demo.gridfields.grid.dc}"
					width="100:100" />
				<combobox bind="carType" fill="ct.carName"
					title="${label.demo.gridfields.grid.ct}"
						  width="120:120"/>

				<phone bind="homePhone" title="${demo.gridfields.grid.home_phone}" width="135:135"/>
				<text bind="remark" title="${label.demo.gridfields.grid.remark}"
					  width="50:1200" />

			</cols>
		</grid>
	</items>
</panel>
